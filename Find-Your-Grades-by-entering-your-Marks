  <script >let num = prompt("Enter Your Marks For finding Grades");

  if (num >= 90) {
    console.log("A");
  }else if(num >= 70){
    console.log("B");
  }else if(num >= 50){
    console.log("C");
  }else if(num >= 30){
    console.log("D");
  }else {
    console.log("Failed");
  }
</script>
